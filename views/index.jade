	.container-fluid
		h1 MyMapNow
		br
		br
		.row
			.col-sm-offset-1
				.map-checkbox-form
					form(method="get")
						.col-md-2.form-group
							label 
								input(type="checkbox", class="all-bing", name="", value="")
								|  Load ALL Bing Maps
							br
							br
							label
								input(type="checkbox", class="bing", name="bing", value="Aerial")
								|  Aerial
							br
							label
								input(type="checkbox", class="bing", name="bing", value="Auto")
								|  Auto
							br
							label
								input(type="checkbox", class="bing", name="bing", value="Birdseye")
								|  Birdseye
							br
							label
								input(type="checkbox" class="bing", name="bing", value="CollinsBart")
								|  Collin's Bart
							br
							label
								input(type="checkbox", class="bing", name="bing", value="Mercator")
								|  Mercator
							br
							label
								input(type="checkbox", class="bing", name="bing", value="OrdnanceSurvey")
								|  Ordnance Survey
							br
							label
								input(type="checkbox", class="bing", name="bing", value="Road")
								|  Road
						.col-md-2.form-group
							label
								input(type="checkbox", class="all-esri", name="", value="")
								|  Load ALL ESRI Maps
							br
							br
							label
								input(type="checkbox", class="esri", name="esri", value="gray")
								|  Gray
							br
							label
								input(type="checkbox", class="esri", name="esri", value="hybrid")
								|  Hybrid
							br
							label
								input(type="checkbox", class="esri", name="esri", value="national-geographic")

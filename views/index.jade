							br
							label
								input(type="checkbox", class="esri", name="esri", value="satellite")
								|  Satellite
							br
							label
								input(type="checkbox", class="esri", name="esri", value="topo")
								|  Topographic
						.col-md-2.form-group
							label
								input(type="checkbox", class="all-google", name="", value="")
								|  Load ALL Google Maps
							br
							br
							label
								input(type="checkbox", class="google", name="google", value="HYBRID")
								|  Hybrid
							br
							label
								input(type="checkbox", class="google", name="google", value="ROAD")
								|  Road
							br
							label
								input(type="checkbox", class="google", name="google", value="SATELLITE")
								|  Satellite
							br
							label
								input(type="checkbox", class="google", name="google", value="TERRAIN")
								|  Terrain
						.col-md-2.form-group
							label
								input(type="checkbox", class="all-mapquest", name="", value="")
								|  Load ALL Mapquest Maps
							br
							br
							label
								input(type="checkbox", class="mapquest", name="mapquest", value="hyb")
								|  Hybrid
							br
							label
								input(type="checkbox", class="mapquest", name="mapquest", value="map")
								|  Map
							br
							label
								input(type="checkbox", class="mapquest", name="mapquest", value="osm")
								|  Open Street Map
							br
							label
								input(type="checkbox", class="mapquest", name="mapquest", value="sat")
								|  Satellite
						.col-md-2.form-group
							label
								input(type="checkbox", class="all-nokia", name="", value="")
								|  Load ALL Nokia Maps
							br
							br
							label
								input(type="checkbox", class="nokia", name="nokia", value="NORMAL")
								|  Normal
							br
							label
								input(type="checkbox", class="nokia", name="nokia", value="NORMAL_COMMUNITY")
								|  Normal Community
							br
							label
								input(type="checkbox", class="nokia", name="nokia", value="SATELLITE")
								|  Satellite
							br
							label
								input(type="checkbox", class="nokia", name="nokia", value="SATELLITE_COMMUNITY")
								|  Satellite Community
							br
							label
								input(type="checkbox", class="nokia", name="nokia", value="SMARTMAP")
								|  Smartmap
							br
							label
								input(type="checkbox", class="nokia", name="nokia", value="SMART_PT")
								|  Smart PT
							br
							label
								input(type="checkbox", class="nokia", name="nokia", value="TERRAIN")
								|  Terrain
							br
							label
								input(type="checkbox", class="nokia", name="nokia", value="TRAFFIC")
								|  Traffic
						.col-md-2.form-group
							label
								input(type="checkbox", class="all-maps", name="", value="")
								|  Load ALL 30 Vendor Map Types
							br
							br
							input(button type="submit" class="btn btn-success")
		.row
			.col-sm-offset-1
				for value in bing
					.map-shell.col-md-2
						.map-wrapper
							.map.bing-type(data-options-type=value)
				for value in esri
					.map-shell.col-md-2
						.map-wrapper
							.map.esri-type(data-options-type=value)
				for value in google
					.map-shell.col-md-2
						.map-wrapper
							.map.google-type(data-options-type=value)
				for value in mapquest
					.map-shell.col-md-2
						.map-wrapper
							.map.mapquest-type(data-options-type=value)
				for value in nokia
					.map-shell.col-md-2
						.map-wrapper
							.map.nokia-type(data-options-type=value)
